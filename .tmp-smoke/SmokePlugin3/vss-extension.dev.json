{
  "manifestVersion": 1,
  "id": "smokeplugin3",
  "publisher": "local-test",
  "version": "0.0.1",
  "name": "SmokePlugin3",
  "description": "SmokePlugin3 Azure DevOps extension",
  "public": false,
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "categories": [
    "Code"
  ],
  "files": [
    {
      "path": "dist",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "smokeplugin3-hub",
      "type": "ms.vss-web.hub",
      "description": "SmokePlugin3 hub",
      "targets": [
        "ms.vss-code-web.code-hub-group"
      ],
      "properties": {
        "name": "SmokePlugin3",
        "uri": "dist/index.html"
      }
    }
  ],
  "baseUri": "https://localhost:3000"
}